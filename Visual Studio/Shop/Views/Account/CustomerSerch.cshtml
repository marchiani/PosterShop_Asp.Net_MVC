@model IEnumerable<Shop.Models.ShopModel.Order>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telephone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeDelivery)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostersThatOrser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataSet)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telephone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeDelivery)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostersThatOrser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataSet)
        </td>
        <td>
            @Ajax.ActionLink("Delete" , "DeleteProduct", new AjaxOptions{ UpdateTargetId = "delete"})
            @Html.ActionLink("Delete", "DeleteProduct", new {id = item.Id})
        </td>
    </tr>
}

</table>
<div id="delete">hi</div>